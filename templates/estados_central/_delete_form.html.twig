<form method="post" action="{{ path('estados_central_delete', {'id': estados_central.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ estados_central.id) }}">
    <button class="btn">Delete</button>
</form>
